<string name="battle_script_strings_title_Rom.HB.TF_Intro_Date">text_rejected</string>
<string name="battle_script_strings_string_Rom.HB.TF_Battle_Lost_01">Our Eagles! We are lost... </string>
<string name="battle_script_strings_string_Rom.HB.TF_Battle_Lost_02">We are lost! Damn you Varus, you spineless coward!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_09">Berserkers! Brace Yourselves!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_08">Go! Get the standards out! The legions do not fail!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_07">Look to the rear! Berserkers!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_06">Rally, men! We're nearly clear of the forest!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_05">Varus! Accursed coward! Push forward while he distracts the enemy!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_04">You… you're on your own. Horsemen - with me! Double-time!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_03">Your orders, General?</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_02">Keep moving! We must get through! Where is Arminius? Someone find Arminius! We need his auxiliaries!</string>
<string name="battle_script_strings_string_Rom.HB.TF_Gameplay_01">We're under attack! Look to the eagles! Defend them! Hold on!</string>
